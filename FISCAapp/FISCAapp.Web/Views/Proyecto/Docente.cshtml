@model IEnumerable<FISCA.Dominio.Entidades.Docente>

<table border="1">
    <thead>
        <tr>
            <th>IdDocente</th>
            <th>NombresDocente</th>
            <th>ApellidosDocente</th>
            <th>CedulaDocente</th>
            <th>CorreoDocente</th>
            <th>CelularDocente</th>
            <th>TelefonoDocente</th>
            <th>DireccionDocente</th>
            <th>Estado</th>
            <th>Foto</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var docente in Model)
        {
            <tr>
                <td>@docente.IdDocente</td>
                <td>@docente.NombresDocente</td>
                <td>@docente.ApellidosDocente</td>
                <td>@docente.CedulaDocente</td>
                <td>@docente.CorreoDocente</td>
                <td>@docente.CelularDocente</td>
                <td>@docente.TelefonoDocente</td>
                <td>@docente.DireccionDocente</td>
                <td>@docente.Estado</td>
                <td>@docente.Foto</td>
            </tr>
        }
    </tbody>
</table>

<style>
    table {
        border-collapse: collapse;
        width: 100%;
    }

    th, td {
        border: 1px solid black;
        padding: 4px;
        text-align: left;
    }

    th {
        background-color: #f2f2f2;
    }
</style>